{
  "name": "yaaas",
  "version": "0.1.3",
  "description": "Yet Another API for Anime and Stuff",
  "engines": {
    "node": ">=20.0 < 21.0"
  },
  "scripts": {
    "start": "pnpm build && node dist/server.js",
    "build": "pnpm fix && rimraf dist && tsc -p tsconfig.json",
    "dev": "nodemon server.ts",
    "lint": "eslint src",
    "format": "prettier src --check",
    "fix": "pnpm format --write && pnpm lint --fix"
  },
  "keywords": [
    "api",
    "scraper",
    "anime",
    "manga",
    "manhwa",
    "anime-api",
    "manga-api",
    "manhwa-api"
  ],
  "homepage": "https://github.com/Aerysh/yaaas",
  "author": "Ahmad Azwar Annas <ahmadazw2@gmail.com>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/node": "^20.12.11",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^0.5.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.27.0",
    "puppeteer": "^22.8.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
